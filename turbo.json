{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".build/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "input": ["./packages/editor-presets"]
    },
    "clean": {
      "cache": false
    },
    "package": {
      "outputs": ["package"]
    }
  }
}
